#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/obrooks/Documents/Projects/owen-brooks.github.io/node_modules/.pnpm/astro@5.11.0_jiti@2.4.2_lightningcss@1.30.1_rollup@4.41.1_typescript@5.8.3_yaml@2.7.0/node_modules/astro/node_modules:/Users/obrooks/Documents/Projects/owen-brooks.github.io/node_modules/.pnpm/astro@5.11.0_jiti@2.4.2_lightningcss@1.30.1_rollup@4.41.1_typescript@5.8.3_yaml@2.7.0/node_modules:/Users/obrooks/Documents/Projects/owen-brooks.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/obrooks/Documents/Projects/owen-brooks.github.io/node_modules/.pnpm/astro@5.11.0_jiti@2.4.2_lightningcss@1.30.1_rollup@4.41.1_typescript@5.8.3_yaml@2.7.0/node_modules/astro/node_modules:/Users/obrooks/Documents/Projects/owen-brooks.github.io/node_modules/.pnpm/astro@5.11.0_jiti@2.4.2_lightningcss@1.30.1_rollup@4.41.1_typescript@5.8.3_yaml@2.7.0/node_modules:/Users/obrooks/Documents/Projects/owen-brooks.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../astro/astro.js" "$@"
else
  exec node  "$basedir/../astro/astro.js" "$@"
fi
